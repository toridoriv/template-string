{
  "bracketSameLine": true,
  "endOfLine": "lf",
  "printWidth": 90,
  "proseWrap": "always",
  "quoteProps": "consistent",
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "plugins": ["@homer0/prettier-plugin-jsdoc"],
  "jsdocUseTypeScriptTypesCasing": false,
  "jsdocIndentFormattedExamples": false,
  "jsdocFormatExamples": false,
  "jsdocFormatStringLiterals": true,
  "jsdocUseSingleQuotesForStringLiterals": false,
  "jsdocFormatComplexTypesWithPrettier": true,
  "jsdocEnsureDescriptionsAreSentences": true,
  "jsdocExperimentalFormatCommentsWithoutTags": true,
  "jsdocTagsOrder": [
    "typedef",
    "callback",
    "function",
    "method",
    "class",
    "file",
    "constant",
    "description",
    "classdesc",
    "example",
    "template",
    "type",
    "param",
    "property",
    "returns",
    "augments",
    "extends",
    "throws",
    "yields",
    "fires",
    "listens",
    "async",
    "abstract",
    "override",
    "private",
    "protected",
    "public",
    "access",
    "desprecated",
    "author",
    "version",
    "since",
    "member",
    "memberof",
    "category",
    "external",
    "see",
    "other",
    "todo"
  ],
  "overrides": [
    {
      "files": ["*.test.ts"],
      "options": {
        "printWidth": 120
      }
    }
  ]
}
